<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.miujike.userservice.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.miujike.userservice.domain.User">
        <id column="user_id" property="userId"/>
        <result column="nickname" property="nickname"/>
        <result column="avatar_url" property="avatarUrl"/>
        <result column="create_time" property="createTime"/>
        <result column="last_login_time" property="lastLoginTime"/>
        <result column="last_sign_time" property="lastSignTime"/>
        <result column="country" property="country"/>
        <result column="province" property="province"/>
        <result column="city" property="city"/>
        <result column="gender" property="gender"/>
        <result column="open_id" property="openId"/>
        <result column="fans" property="fans"/>
        <result column="follows" property="follows"/>
        <result column="thumbs" property="thumbs"/>
        <result column="eggs" property="eggs"/>
    </resultMap>
    <sql id="Base_Column_List">
        user_id as userId,nickname,avatar_url as avatarUrl,create_time as createTime,last_login_time as lastLoginTime,country,province,city,gender,open_id as openId,
        fans,follows,thumbs,eggs
    </sql>
</mapper>